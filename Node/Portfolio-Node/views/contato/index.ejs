<section id="contato">
    <div class="container">
      <h1 class="title">Entre em contato</h1>
      <hr>
      <div class="container-formulario">
        <form class="formulario" action="/contato" method="POST">
          <p>Digite seu nome:</p>
          <input
            type="text"
            name="nome"
            id="nome"
            placeholder="Nome Completo"
            required
          />
          <p>Digite seu e-mail para contato:</p>
          <input
            type="email"
            name="email"
            id="email"
            placeholder="Email"
            required
          />
          <p>Assunto:</p>
          <input
            type="text"
            name="assunto"
            id="assunto"
            placeholder="Assunto"
            required
          />
          <p>Mensagem:</p>
          <textarea
            name="mensagem"
            id="mensagem"
            placeholder="Digite sua mensagem aqui"
            cols="30"
            rows="8"
            required
          ></textarea>
          <input type="hidden" name="_csrf" value="<%= data.csfrToken %>" />
          <input type="submit" value="Enviar" class="btn" />
        </form>
      </div>
    </div>
  </section>