<div class="title">
  <h1>FlappyBird</h1>
  <hr>
</div>

<div id="jogo" class="jogo">
  <div id="jogo-container" class="jogo-container flappybird">
    <div id="formulario-container" class="formulario-container">
      <form id="game-formulario" class="formulario" method="POST">
        <h3>Digite um nickname</h2>
          <input type="text" name="nome" placeholder="Digite um nickname" id="nome" required />
          <input type="hidden" name="_csrf" value="<%= data.csfrToken %>" />
          <input type="submit" class="btn" />
      </form>
    </div>

    <div id="game-screen" class="game-screen">
      <canvas id="hud-layer"></canvas>
      <canvas id="game-layer"></canvas>
      <canvas id="ground-layer"></canvas>
      <canvas id="pipeSouth-layer"></canvas>
      <canvas id="background-layer"></canvas>
    </div>

    <div id="loading-txt"></div>
  </div>
</div>